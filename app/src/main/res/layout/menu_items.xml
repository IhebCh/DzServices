<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:circle="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <com.itech.dzservices.CircleImageView
        android:id="@+id/main_home_reparation_image"
        circle:name="@string/home_reparation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/home_reparation" />


    <com.itech.dzservices.CircleImageView
        android:id="@+id/main_maintenance_info_image"
        circle:name="@string/maintenance_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/maintenance_info" />


    <com.itech.dzservices.CircleImageView
        android:id="@+id/main_reparation_car_image"
        circle:name="@string/reparation_car"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/reparation_car" />

    <com.itech.dzservices.CircleImageView
        android:id="@+id/main_reparation_tel_image"
        circle:name="@string/reparation_tel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/reparation_tel" />

    <com.itech.dzservices.CircleImageView
        android:id="@+id/main_transport_image"
        circle:name="@string/transport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/transport" />

</merge>